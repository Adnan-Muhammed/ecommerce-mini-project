        data: chartData,
        options: {
            responsive: true
        }
    });

    // Filter function
    function filterData(country) {
        var newData = chartData.datasets.filter(function(dataset) {
            return dataset.label === country;
        });

        if (newData.length > 0) {
            myChart.data.datasets = newData;
            myChart.update();
        } else {
            console.log("Country data not found!");
        }
    }

    // Example usage: filterData("USA");
});
</script>


