<script>
    var ctx1 = $("#worldwide-sales2").get(0).getContext("2d");
    var chartData = {
        labels: ["2016", "2017", "2018", "2019", "2020", "2021", "2022"],
        datasets: [{
                label: "Year",
                data: [15, 30, 55, 65, 60, 80, 95],
                backgroundColor: "rgba(0, 156, 255, .7)"
            },
            {
                label: "Month",
                data: [8, 35, 40, 60, 70, 55, 75, 40, 70, 39, 68, 55],
                backgroundColor: "rgba(0, 156, 255, .5)"
            }
        ]
    };

    var myChart1 = new Chart(ctx1, {
        type: "bar",
        data: chartData,
        options: {
            responsive: true
        }
    });

    // Filter function
    function filterData(filterType) {
        if (filterType === 'year') {
            chartData.labels = ["2016", "2017", "2018", "2019", "2020", "2021", "2022"];
            myChart1.data.datasets[0].label = "Year";
        } else if (filterType === 'month') {
            chartData.labels = ["January", "February", "March", "April", "May", "June", "July", "August", "September", "October", "November", "December"];
            myChart1.data.datasets[0].label = "Month";
        }
        myChart1.update();
    }

    // Example usage: filterData("month");
</script>



